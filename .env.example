# Database Configuration
DATABASE_URL="mysql://user:password@host:port/database"

# Google OAuth Configuration
GOOGLE_CLIENT_ID="your-google-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# JWT Configuration
JWT_SECRET="your-jwt-secret-key"

# Application Configuration
EXPO_PUBLIC_APP_ID="buscazap"
VITE_APP_ID="buscazap"
NODE_ENV="production"
PORT="8080"

# Public API URL (without http/https prefix for Railway)
PUBLIC_API_URL="your-app.up.railway.app"
EXPO_PUBLIC_API_BASE_URL="https://your-app.up.railway.app"

# Owner Configuration (optional)
OWNER_OPEN_ID="your-google-user-id"

# Expo Configuration (for mobile app)
EXPO_ACCESS_TOKEN="your-expo-access-token"
